---
import Section from "@/layouts/Section.astro"
import { SECTIONS_IDS } from "@/helpers/adaptHeaderItems"
import { Projects as ProjectsComponent } from "@/components/ui"

const { i18n } = Astro.props as { i18n: PROJECTS }
---

<Section 
  id={SECTIONS_IDS[2]}
  maxWidth="1200px"
  class="flex flex-col justify-center items-baseline"
  animated={true}
>
  <ProjectsComponent
    client:load
    title={i18n.TITLE}
    projects={i18n.PROJECTS_LIST}
  />
</Section>